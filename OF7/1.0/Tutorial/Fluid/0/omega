FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 100;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    "wall.*"
    {
        //type            fixedValue;
        //value		uniform 4.15e8;//1.04e8;
        type            omegaWallFunction;
	//blended 	false;
       value		$internalField;//
    }
    frontAndBack
    {
        type            empty;
    }
}


